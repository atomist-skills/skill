{
  "name": "@atomist/skill",
  "version": "0.11.1",
  "description": "Atomist Skill API",
  "keywords": [
    "automation"
  ],
  "homepage": "https://github.com/atomist-skills/skill",
  "bugs": {
    "url": "https://github.com/atomist-skills/skill/issues"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Atomist, Inc.",
    "email": "oss@atomist.com",
    "url": "https://atomist.com"
  },
  "main": "./index.js",
  "browser": {
    "atm-skill": "bin/start.js"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atomist-skills/skill.git"
  },
  "scripts": {
    "atm:lint:eslint": "eslint --fix",
    "atm:lint:prettier": "prettier --write",
    "autotest": "supervisor --watch index.ts,lib,test --extensions ts --no-restart-on exit --quiet --exec npm -- test",
    "build": "run-s clean compile test lint doc",
    "clean": "run-p clean:compile clean:test clean:doc clean:run",
    "clean:compile": "rimraf git-info.json \"*.{d.ts,js}{,.map}\" \"{bin,lib,test}/**/*.{d.ts,js}{,.map}\" lib/typings/types.ts",
    "clean:dist": "run-s clean clean:npm",
    "clean:doc": "rimraf doc",
    "clean:npm": "rimraf node_modules",
    "clean:run": "rimraf *-v8.log profile.txt log",
    "clean:test": "rimraf .nyc_output coverage",
    "compile": "run-s git:info gql:gen compile:ts",
    "compile:ts": "tsc --project .",
    "doc": "typedoc --mode modules --excludeExternals --ignoreCompilerErrors --exclude \"**/*.d.ts\" --out doc index.ts lib",
    "git:info": "atm-npm-tools git-info",
    "gql:gen": "graphql-codegen",
    "lint": "run-p lint:eslint lint:prettier",
    "lint:eslint": "eslint --ext .ts .",
    "lint:eslint:fix": "npm run lint:eslint -- --fix",
    "lint:fix": "run-p lint:eslint:fix lint:prettier:fix",
    "lint:prettier": "prettier --list-different \"**/*.{graphql,json,markdown,md,yaml,yml}\"",
    "lint:prettier:fix": "prettier --write \"**/*.{graphql,json,markdown,md,yaml,yml}\"",
    "prepack": "atm-npm-tools source-maps",
    "schema": "node ./bin/start.js gql-fetch",
    "test": "mocha --require espower-typescript/guess \"test/**/*.test.ts\"",
    "test:one": "mocha --require espower-typescript/guess \"test/**/${TEST:-*.test.ts}\""
  },
  "dependencies": {
    "@atomist/skill-logging": "^1.0.8-main.8",
    "@atomist/slack-messages": "^1.2.2",
    "@google-cloud/pubsub": "^2.14.0",
    "@google-cloud/storage": "^5.8.5",
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.3",
    "@graphql-codegen/typescript-operations": "^1.18.2",
    "@octokit/plugin-rest-endpoint-methods": "^5.3.1",
    "@octokit/plugin-retry": "^3.0.8",
    "@octokit/plugin-throttling": "^3.5.0",
    "@octokit/rest": "^18.6.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/flat": "^5.0.1",
    "@types/fs-extra": "^9.0.11",
    "@types/git-url-parse": "^9.0.0",
    "@types/js-yaml": "^4.0.1",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.forown": "^4.4.6",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/lodash.map": "^4.6.13",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.set": "^4.3.6",
    "@types/lodash.sortby": "^4.7.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/node-fetch": "^2.5.10",
    "@types/pluralize": "0.0.29",
    "@types/semver": "^7.3.6",
    "@types/stack-trace": "0.0.29",
    "@types/uuid": "^8.3.0",
    "@types/yargs": "^17.0.0",
    "@vercel/ncc": "^0.28.6",
    "cross-spawn": "^7.0.3",
    "edn-data": "^0.2.0",
    "fast-glob": "^3.2.5",
    "fastest-levenshtein": "^1.0.12",
    "find-up": "^5.0.0",
    "flat": "^5.0.2",
    "fs-extra": "^10.0.0",
    "git-url-parse": "^11.4.4",
    "glob-gitignore": "^1.0.14",
    "graphql": "^15.5.1",
    "graphql-tag": "^2.12.4",
    "handlebars": "^4.7.7",
    "jose": "^3.13.0",
    "js-yaml": "^4.1.0",
    "jszip": "^3.6.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.chunk": "^4.2.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.forown": "^4.4.0",
    "lodash.kebabcase": "^4.1.1",
    "lodash.map": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "lodash.set": "^4.3.2",
    "lodash.sortby": "^4.7.0",
    "lodash.uniq": "^4.5.0",
    "node-fetch": "^2.6.1",
    "p-retry": "^4.5.0",
    "pluralize": "^8.0.0",
    "prettier": "^2.3.1",
    "semver": "^7.3.5",
    "source-map-support": "^0.5.19",
    "stack-trace": "0.0.10",
    "strip-ansi": "^7.0.0",
    "tree-kill": "^1.2.2",
    "uuid": "^8.3.2",
    "word-wrap": "^1.2.3",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@atomist/npm-tools": "^0.1.1-main.0",
    "@types/mocha": "^8.2.2",
    "@types/power-assert": "^1.5.3",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "espower-typescript": "^10.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mocha": "^8.4.0",
    "npm-run-all": "^4.1.5",
    "power-assert": "^1.6.1",
    "rimraf": "^3.0.2",
    "supervisor": "^0.12.0",
    "ts-node": "^10.0.0",
    "typedoc": "^0.20.37",
    "typescript": "^4.3.4"
  },
  "engines": {
    "node": ">=8.2.0",
    "npm": ">=5.0.0"
  },
  "bin": {
    "atm-skill": "bin/start.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.@(graphql|json|markdown|yaml|yml|md)": "npm run atm:lint:prettier",
    "**/*.ts": "npm run atm:lint:eslint"
  },
  "types": "./index.d.ts"
}
